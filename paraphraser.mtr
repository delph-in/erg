look+up := monotonic_mtr &
[ INPUT.RELS <! [ PRED "_look_v_up_rel", LBL #h1, 
                  ARG0 #e2, ARG1 #x3, ARG2 #x4, ARG3 #x5, ARG4 #x6 ],
                [ PRED _up_p_sel_rel, LBL #h1, ARG1 #e2, ARG2 #x4 ] !>,
  OUTPUT.RELS <! [ PRED "_find_v_1_rel", LBL #h1, 
                   ARG0 #e2, ARG1 #x3, ARG2 #x4, ARG3 #x5, ARG4 #x6 ] !> ].

