;;; take advantage of new constant code

arbitrary_quantifier_restriction_rule2 := mrs_rule &
 [ INPUT [ LISZT <! ^c20 & 
                   [ HANDEL #h1,
                     BV #x2,
                     RESTR #h3,
		     SCOPE #h6,
                     DIM #v10 ],
		    ^c21 &
                   [ HANDEL #h5,
                     BV #x12,
                     RESTR #h14,
                     SCOPE #h15,
                     DIM #v20 ] !>,
           H-CONS <! [ SC-ARG #h3,
                       CANDS <! #h1,#h4,#h5 !> ] !> ],
  OUTPUT [ LISZT <! ^c20 &
                   [ HANDEL #h1,
                     BV #x2,
                     RESTR #h4,
                     SCOPE #h6,
                     DIM #v10 ],
		    ^c21 &
                   [ HANDEL #h5,
                     BV #x12,
                     RESTR #h14,
                     SCOPE #h15,
                     DIM #v20 ] !>,
           H-CONS <! !> ] ] .

