		     KEYS.KEY non_temp_nom_rel ],
	     MODIFIED notmod ] ].

n_intr_le :< n_intr_lex_entry.
n_intr_lowprio_le :< n_intr_lex_entry.

n_intr_temp_le := basic_intr_noun_word & 
