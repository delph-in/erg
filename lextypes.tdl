generic_n_intr_lex_entry := unknown_word &
  [ SYNSEM mass_or_count_ppcomp_synsem & 
	   [ LOCAL [ CAT.VAL.SPR < [ LOCAL.KEYS.KEY quant_or_wh_rel ] >,
                     CONT.LISZT <! #key !>,
		     KEYS.KEY #key & generic_noun_rel ],
