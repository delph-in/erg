  [ LOCAL [ CAT.VAL.--KEYCOMP.LOCAL local_min &
				    [ KEYS.MESSAGE 1-dlist & <! hypo_rel !>,
				      CONT.TOP #hand ],
	    KEYS.KEY.ARG4 #hand ] ].

subj_equi_prp_lt := subj_equi_lt &
  [ LOCAL [ CAT.VAL.--KEYCOMP.LOCAL local_min &
				    [ CONT.TOP #hand ],
	    CONT [ LISZT <! relation, hypo_rel & [ HANDEL #chand,
                                                   SOA #shand ] !>,
                   H-CONS <! qeq &
			   [ SC-ARG #shand,
			     OUTSCPD #hand ] !> ],
	    KEYS.KEY.ARG4 #chand ] ].

