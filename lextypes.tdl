adj_reg_atrans_non-inf_cp_le := reg_adj_atrans_cp_word &
  [ SYNSEM reg_adj_atrans_non-inf_cp_synsem ].

adj_reg_atrans_whether_cp_le := reg_adj_atrans_cp_word &
