			    [ LOCAL [ CAT s_cat_v_c,
				      CONJ cnil ] ] > ] ]. 
  
pp_cp_subst := three_arg_subst &
  [ LOCAL.CAT.VAL [ --KEYCOMP #comp,
		    COMPS < synsem &
			    [ LOCAL [ CAT pp_cat,
				      CONJ cnil ] ],
			    #comp & synsem &
			    [ LOCAL [ CAT s_cat_fin_v_c,
				      CONJ cnil ] ] > ] ].
  
np_comp_subst := three_arg_trans_subst & 
