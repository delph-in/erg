subj_equi_prp_verb := verb_synsem & prp_intrans_subst & subj_equi_prp_lt.

; "K became quiet"
subj_prd_verb := verb_synsem & ssr_two_arg_subst & prd_comp_lt &
  [ LOCAL.CAT.VAL [ COMPS < synsem & #comp &
                            [ LOCAL [ CAT prd_cat,
                                      CONJ cnil ] ] >,
                    --KEYCOMP #comp ] ].

