  [ SYNSEM np_trans_verb &
	   [ LOCAL [ CAT.VAL.COMPS < [ OPT + ] >,
		     CONT.LISZT <! #key !>,
                     KEYS.KEY #key & generic_verb_rel ] ] ].

v_np*_trans_bse_nale := generic_v_np*_trans_lex_entry &
