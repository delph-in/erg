                              two_arg & arg14_lt.

cp_prop_intrans_verb := cp_intrans_verb &
  [ LOCAL.CAT.VAL.COMPS < [ LOCAL.KEYS.MESSAGE 1-dlist &
					       <! prpstn_rel !> ] > ].

cp_ques_intrans_verb := cp_intrans_verb &
  [ LOCAL.CAT.VAL.COMPS < [ LOCAL.KEYS.MESSAGE 1-dlist &
					       <! int_rel !> ] > ].

;; suppose
