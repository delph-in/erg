;;; redefine basic_relation adding cfrom and cto
;;; for characterization experiments

initial-cfrom-val := atom.

initial-cto-val := atom.

string := initial-cfrom-val & initial-cto-val.

basic_relation := *avm* &
  [ LBL handle,
    WLINK *list*,
    PRED predsort,
    CFROM initial-cfrom-val,
    CTO initial-cto-val ].

testfoobar := basic_relation &
  [ CFROM "1",
    CTO "2" ].
