vocative_np_phrase := norm_unary_phrase & phrasal &
 [ INFLECTD +,
   SYNSEM disc_adv_phr_synsem &
            [ LOCAL [ CAT [ HEAD [ PRD -,
                                   MOD < [ LOCAL scopal_mod &
                                            [ CAT [ HEAD adverbee &
                                                      [ TAM.TENSE real_tense ],
                                                    VAL.SUBJ *olist* ],
                                              CONT.HOOK [ LTOP #ltop,
                                                          INDEX #index ] ] ] >,
                                   MINORS.MIN abstr_excl_rel ],
                            MC + ],
                      CONJ #conj ],
              NONLOC #nonloc ],
   ARGS < [ INFLECTD +,
            SYNSEM [ LOCAL [ CAT [ HEAD noun & 
                                        [ MINORS [ MIN named_rel,
                                                   ALTMIN proper_q_rel ] ],
                                   VAL [ SPR *olist*,
                                         COMPS < >,
