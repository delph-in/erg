# First append the robust irregs file to the standard one, dropping the final
# double-quote from the standard file
cp irregs.tab /tmp/irregs.tab
head -n -1 irregs.tab > /tmp/irregs; cat /tmp/irregs essay/irregs-robust.tab > irregs.tab
ace -G erg.dat -g ace/config-essay-full.tdl
cp /tmp/irregs.tab .

