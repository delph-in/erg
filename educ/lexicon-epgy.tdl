; Pronouns with mismatched case

him_rbst := n_-_pr-he_le &
 [ ORTH < "him" >,
   SYNSEM [ PHON.ONSET con,
            LOCAL.CAT.HEAD.MINORS.NORM no_rel ],
   GENRE robust ].

